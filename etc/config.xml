<?xml version="1.0"?>

<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <payment>
            <fgc_nabevm3d>
                <title>NAB 3D Secure</title>
                <model>Fgc\NabEvm3D\Model\NabEvm3D</model>
                <active>1</active>
                <useccv>0</useccv>
                <cctypes>AE,VI,MC,DI</cctypes>
                <merchant_id/>
                <merchant_password backend_model="Magento\Config\Model\Config\Backend\Encrypted" />
                <order_status>pending_payment</order_status>
                <sandbox_mode>1</sandbox_mode>
                <liability_shift>issuer_and_merchant</liability_shift>
                <payment_action>authorize_capture</payment_action>
                <currency>AUD</currency>
                <can_authorize>1</can_authorize>
                <can_capture>1</can_capture>
                <can_void>1</can_void>
                <can_use_checkout>1</can_use_checkout>
                <is_gateway>1</is_gateway>
                <sort_order>1</sort_order>
            </fgc_nabevm3d>
        </payment>
    </default>
</config>

